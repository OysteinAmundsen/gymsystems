  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "Client",
      "preLaunchTask": "Client serve",
      "sourceMaps": true,
      "webRoot": "${workspaceRoot}",
      "url": "http://localhost:4200"
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Client Test",
      "sourceMaps": true,
      "webRoot": "${workspaceRoot}",
      "url": "http://localhost:9876/debug.html",
      "runtimeArgs": [
        "--headless"
      ]
    }
  ]
}
