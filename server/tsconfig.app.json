{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "baseUrl": "",
    "noLib": false,
    "rootDir": "./",
    "module": "commonjs",
    "outDir": "../dist/",
    "target": "es6",
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitUseStrict": false,
    "noFallthroughCasesInSwitch": true
  },
  "exclude": [
    "node_modules",
    "**/*.spec.ts"
  ]
}
