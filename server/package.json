{
  "name": "gymsystems_server",
  "version": "2.0.0",
  "license": "MIT",
  "author": "oystein.amundsen@gmail.com",
  "scripts": {
    "nest": "nest",
    "typeorm": "typeorm",
    "tsc": "tsc",
    "build": "nest i && tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "concurrently --kill-others \"tsc -w\" \"nodemon --config nodemon.json\"",
    "start:debug": "nodemon --config nodemon-debug.json",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:debug": "env NODE_ENV=test node --inspect ../node_modules/jest/bin/jest.js --watch",
    "test:watch": "env NODE_ENV=test jest --watch",
    "test:cov": "env NODE_ENV=test jest --coverage",
    "test:e2e": "env NODE_ENV=test jest --config ./test/jest-e2e.json",
    "migration": "ts-node typeorm migration:run"
  },
  "repository": {
    "type": "git"
  },
  "files": [
    "README.md",
    "dist/src",
    "dist/index*",
    "src"
  ],
  "dependencies": {
    "@nestjs/common": "^5.7.3",
    "@nestjs/core": "^5.7.3",
    "@nestjs/graphql": "^5.5.4",
    "@nestjs/jwt": "^0.3.0",
    "@nestjs/passport": "^5.1.0",
    "@nestjs/swagger": "^2.5.1",
    "@nestjs/typeorm": "^5.3.0",
    "apollo-server-express": "^2.4.7",
    "bcrypt": "^3.0.4",
    "class-transformer": "^0.2.0",
    "class-validator": "^0.9.1",
    "cls-hooked": "^4.2.2",
    "concurrently": "^4.1.0",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "fast-csv": "^2.4.1",
    "graphql": "^14.1.1",
    "graphql-subscriptions": "^1.0.0",
    "graphql-tools": "^4.0.4",
    "graphql-type-json": "^0.2.1",
    "handlebars": "^4.1.0",
    "helmet": "^3.15.1",
    "json2csv": "^4.3.5",
    "lodash": "^4.17.11",
    "mkdirp": "^0.5.1",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "morgan-body": "^2.4.5",
    "mysql": "^2.16.0",
    "mysqldump": "^2.2.1",
    "node-schedule": "^1.3.2",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^2.6.3",
    "rotating-file-stream": "^1.4.0",
    "rxjs": "^6.4.0",
    "sendmail": "^1.4.1",
    "typeorm": "^0.2.14",
    "v8-compile-cache": "^2.0.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^5.8.0",
    "@nestjs/testing": "^5.7.3",
    "@types/bcrypt": "^3.0.0",
    "@types/cls-hooked": "^4.2.1",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.1",
    "@types/graphql": "^14.0.7",
    "@types/handlebars": "^4.0.40",
    "@types/helmet": "0.0.42",
    "@types/jest": "^24.0.9",
    "@types/json2csv": "^4.2.0",
    "@types/lodash": "^4.14.121",
    "@types/morgan": "^1.7.35",
    "@types/node": "^11.9.5",
    "@types/passport": "^1.0.0",
    "@types/passport-jwt": "^3.0.1",
    "@types/supertest": "^2.0.7",
    "jest": "^24.1.0",
    "jest-dot-reporter": "^1.0.7",
    "nodemon": "^1.18.10",
    "supertest": "^3.4.2",
    "ts-jest": "^24.0.0",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.2",
    "tsconfig-paths": "^3.8.0",
    "tslint": "5.13.0",
    "typescript": "~3.2.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
