{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": "",
    "noLib": false,
    "rootDir": "./",
    "module": "commonjs",
    "outDir": "../out-tsc/spec",
    "target": "es5",
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitUseStrict": false,
    "noFallthroughCasesInSwitch": true
  },
  "files": [
    "test.ts"
  ],
  "include": [
    "**/*.spec.ts",
    "**/*.d.ts"
  ]
}
