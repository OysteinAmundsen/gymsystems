{
  "name": "gymsystems",
  "version": "1.1.0",
  "description": "Web system for competitive teamgym scoreboarding",
  "homepage": "https://www.gymsystems.org",
  "main": "dist/index.js",
  "bugs": {
    "url": "https://github.com/OysteinAmundsen/gymsystems/issues"
  },
  "repository": "git+https://github.com/OysteinAmundsen/gymsystems",
  "author": "oystein.amundsen@gmail.com",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "ng": "ng",
    "orm": "typeorm",
    "start": "yarn server",
    "lint": "ng lint",
    "clean": "rm -rf dist",
    "test": "yarn server:test && yarn client:test && yarn e2e",
    "cloc": "npx cloc ./src/client > ./src/client/LOC.txt && npx cloc ./src/server > ./src/server/LOC.txt && npx cloc ./e2e > ./e2e/LOC.txt",
    "i18n": "ngx-translate-extract -i ./src/client/app -o ./src/client/i18n/template.json ./src/client/i18n/no.json ./src/client/i18n/en.json --clean --sort",
    "build": "yarn server:build & yarn client:build",
    "update": "ng update",
    "deploy": "yarn clean && yarn build && yarn docker:prod",
    "ngrok": "npx ngrok http 3000 -host-header=\"localhost:3000\"",
    "dev": "concurrently --kill-others --exitcrash -p \"[{name}]\" -n \"SERVER,BUILD\" -c \"bgBlue.bold,bgYellow.bold\" \"yarn server:watch\" \"yarn server:buildw\"",
    "server": "node ./dist/index.js",
    "server:watch": "nodemon --delay 2 dist/index.js -w dist",
    "server:build": "tsc --project ./src/server/tsconfig.app.json",
    "server:buildw": "yarn server:build -w",
    "server:test": "jasmine-ts",
    "client": "ng serve --proxy-config proxy.conf.json",
    "client:prod": "ng serve --proxy-config proxy.conf.json",
    "client:build": "ng --version && ng build --prod --progress=false",
    "client:buildw": "yarn client:build -w",
    "client:test": "yarn client:testw --watch=false",
    "client:testw": "ng test --progress=false",
    "pree2e": "yarn docker:run:test && wait-on tcp:3305 && yarn server:build && typeorm schema:sync -c test && typeorm migration:run -c test",
    "e2e": "concurrently -s first --kill-others --exitcrash -p \"[{name}]\" -n \"SERVER,CLIENT\" -c \"bgBlue.bold,bgYellow.bold\" \"yarn e2e:server\" \"yarn e2e:client\"",
    "e2e:server": "yarn server -- test",
    "e2e:client": "ng e2e --progress=false --proxy-config proxy.conf.json",
    "poste2e": "yarn docker:stop:test",
    "db:clean": "typeorm schema:drop",
    "db:sync": "typeorm schema:sync && yarn migrations:run",
    "migrations:run": "typeorm migration:run",
    "migrations:revert": "typeorm migration:revert",
    "snyk-protect": "snyk protect",
    "prepublish": "yarn snyk-protect",
    "docker:prod": "yarn docker:build:prod && yarn docker:stop:prod && yarn docker:run:prod",
    "docker:dev": "yarn docker:build:dev && yarn docker:stop:dev && yarn docker:run:dev",
    "docker:build:dev": "docker-compose -f ./docker/dev/docker-compose.yml build",
    "docker:build:prod": "docker-compose -f ./docker/prod/docker-compose.yml build",
    "docker:run:db": "docker-compose -f ./docker/db/docker-compose.yml up -d",
    "docker:run:test": "docker-compose -f ./docker/test/docker-compose.yml up -d",
    "docker:run:dev": "docker-compose -f ./docker/dev/docker-compose.yml up",
    "docker:run:prod": "docker-compose -f ./docker/prod/docker-compose.yml up -d",
    "docker:stop:db": "docker-compose -f ./docker/db/docker-compose.yml rm -s -f",
    "docker:stop:test": "docker-compose -f ./docker/test/docker-compose.yml rm -s -f",
    "docker:stop:dev": "docker-compose -f ./docker/dev/docker-compose.yml rm -s -f",
    "docker:stop:prod": "docker-compose -f ./docker/prod/docker-compose.yml rm -s -f"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "pre-push": "yarn server:test && yarn client:test"
    }
  },
  "dependencies": {
    "@agm/core": "1.0.0-beta.3",
    "@angular/animations": "6.0.3",
    "@angular/cdk": "6.1.0",
    "@angular/common": "6.0.3",
    "@angular/compiler": "6.0.3",
