{
  "name": "gymsystems",
  "version": "0.0.0",
  "description": "Web system for competitive teamgym scoreboarding",
  "homepage": "http://www.gymsystems.org",
  "bugs": "https://github.com/OysteinAmundsen/gymsystems/issues",
  "repository": {
    "url": "https://github.com/OysteinAmundsen/gymsystems",
    "type": "git"
  },
  "author": "oystein.amundsen@gmail.com",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=7.7.0"
  },
  "scripts": {
    "help": "node node_modules/npm-scripts-help",
    "ng": "ng",
    "start": "yarn server",
    "lint": "ng lint",
    "clean": "rm -rf dist",
    "cloc": "cloc client > client/LOC.txt && cloc server > server/LOC.txt && cloc e2e > e2e/LOC.txt",
    "i18n": "ngx-translate-extract -i ./client/app -o ./client/i18n/template.json ./client/i18n/no.json ./client/i18n/en.json --clean --sort",
    "build": "yarn server:build & yarn client:build",
    "deploy": "yarn run clean && yarn build && yarn run docker",
    "predev": "yarn server:build",
    "dev": "concurrently --kill-others \"yarn server:buildw\" \"yarn server:watch\"",
    "docker": "sh docker-build",
    "docker:dev": "docker-compose build && docker-compose up -d",
    "server": "node ./dist/index.js",
    "server:watch": "nodemon --delay .2 ./dist/index.js -w dist",
    "server:build": "tsc --project ./server/tsconfig.app.json",
    "server:buildw": "yarn server:build -- -w",
    "server:test": "mocha -r ts-node/register server/**/*.spec.ts",
    "client": "ng serve --proxy-config proxy.conf.json",
    "client:build": "ng build --prod",
    "client:buildw": "yarn client:build -- -w",
    "client:test": "ng test --single-run=true --watch=false",
    "client:testw": "ng test",
    "pree2e": "yarn e2e:docker && wait-on tcp:3305 && yarn server:build && typeorm schema:sync -c test && typeorm migrations:run -c test",
    "e2e": "concurrently -s first --kill-others \"yarn e2e:server\" \"yarn e2e:client\"",
    "e2e:server": "yarn server -- test",
    "e2e:client": "ng e2e --progress=false --proxy-config proxy.conf.json",
    "e2e:docker": "docker-compose -f docker-compose.e2e.yml build && docker-compose -f docker-compose.e2e.yml up -d",
    "poste2e": "docker-compose -f docker-compose.e2e.yml kill",
    "db:clean": "typeorm schema:drop",
    "db:sync": "typeorm schema:sync && yarn migrations",
    "migrations": "typeorm migrations:run",
    "migrations:revert": "typeorm migrations:revert"
  },
  "dependencies": {
    "@angular/common": "^4.3.1",
    "@angular/compiler": "^4.3.1",
    "@angular/core": "^4.3.1",
    "@angular/forms": "^4.3.1",
    "@angular/http": "^4.3.1",
    "@angular/platform-browser": "^4.3.1",
    "@angular/platform-browser-dynamic": "^4.3.1",
    "@angular/router": "^4.3.1",
    "@ngx-translate/core": "^7.1.0",
    "@ngx-translate/http-loader": "0.1.0",
    "angulartics2": "^2.2.2",
    "bcrypt": "^1.0.2",
    "body-parser": "^1.17.2",
    "chalk": "^2.0.1",
    "cookie-parser": "~1.4.3",
    "core-js": "^2.4.1",
    "dragula": "~3.7.2",
    "event-source-polyfill": "0.0.9",
    "express": "~4.15.3",
    "express-session": "^1.15.4",
    "font-awesome": "~4.7.0",
    "handlebars": "^4.0.10",
    "lodash": "^4.17.4",
    "markdown-to-html-pipe": "1.2.1",
    "mkdirp": "~0.5.1",
    "moment": "~2.18.1",
    "morgan": "^1.8.2",
    "multer": "^1.3.0",
    "mysql": "^2.14.0",
    "ng2-dragula": "~1.5.0",
    "node-schedule": "^1.2.3",
    "passport": "~0.3.2",
    "passport-local": "~1.0.0",
    "reflect-metadata": "~0.1.10",
    "rimraf": "^2.6.1",
    "routing-controllers": "0.7.1",
    "rxjs": "^5.4.2",
    "sendmail": "^1.1.1",
    "serve-favicon": "~2.4.3",
    "session-memory-store": "^0.2.2",
    "typedi": "~0.5.2",
    "typeorm": "^0.0.11",
    "winston": "~2.3.1",
    "zone.js": "0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "~1.2.4",
    "@angular/compiler-cli": "^4.3.1",
    "@biesbjerg/ngx-translate-extract": "^2.3.2",
    "@types/bcrypt": "^1.0.0",
    "@types/body-parser": "^1.16.4",
    "@types/bunyan": "^1.8.0",
    "@types/chalk": "~0.4.31",
    "@types/cookie-parser": "~1.4.0",
    "@types/express": "~4.0.36",
    "@types/express-session": "1.15.1",
    "@types/handlebars": "^4.0.35",
    "@types/jasmine": "2.5.53",
    "@types/lodash": "^4.14.71",
    "@types/mkdirp": "~0.5.0",
    "@types/morgan": "~1.7.32",
    "@types/multer": "1.3.2",
    "@types/node": "~8.0.16",
    "@types/node-schedule": "^1.2.0",
    "@types/passport": "~0.3.3",
    "@types/passport-local": "~1.0.30",
    "@types/serve-favicon": "~2.2.28",
    "@types/rimraf": "^0.0.28",
    "@types/winston": "~2.3.4",
    "cloc": "^2.2.0",
    "codelyzer": "~3.1.2",
    "concurrently": "~3.5.0",
    "jasmine-core": "~2.7.0",
    "jasmine-spec-reporter": "~4.1.1",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "nodemon": "~1.11.0",
    "npm-scripts-help": "^0.7.0",
    "protractor": "~5.1.2",
    "ts-node": "^3.3.0",
    "tslint": "~5.5.0",
    "typescript": "^2.4.2",
    "wait-on": "^2.0.2"
  }
}
