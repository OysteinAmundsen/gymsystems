{
  "name": "gymsystems",
  "version": "1.0.1",
  "description": "Web system for competitive teamgym scoreboarding",
  "homepage": "https://www.gymsystems.org",
  "main": "dist/index.js",
  "bugs": {
    "url": "https://github.com/OysteinAmundsen/gymsystems/issues"
  },
  "repository": "git+https://github.com/OysteinAmundsen/gymsystems",
  "author": "oystein.amundsen@gmail.com",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "help": "node node_modules/npm-scripts-help",
    "ng": "ng",
    "start": "yarn server",
    "lint": "ng lint",
    "clean": "rm -rf dist",
    "test": "yarn server:test && yarn client:test && yarn e2e",
    "cloc": "npx cloc client > client/LOC.txt && npx cloc server > server/LOC.txt && npx cloc e2e > e2e/LOC.txt",
    "i18n": "ngx-translate-extract -i ./client/app -o ./client/i18n/template.json ./client/i18n/no.json ./client/i18n/en.json --clean --sort",
    "build": "yarn server:build & yarn client:build",
    "deploy": "yarn clean && yarn build && yarn docker",
    "dev": "concurrently --kill-others --exitcrash -p \"[{name}]\" -n \"SERVER,BUILD\" -c \"bgBlue.bold,bgYellow.bold\" \"yarn server:watch\" \"yarn server:buildw\"",
    "docker": "sh docker-build",
    "docker:dev": "docker-compose build && docker-compose up -d",
    "server": "node ./dist/index.js",
    "server:watch": "nodemon --delay .2 dist/index.js -w dist",
    "server:build": "tsc --project ./server/tsconfig.app.json",
    "server:buildw": "yarn server:build -w",
    "server:test": "jasmine-ts",
    "client": "ng serve --proxy-config proxy.conf.json",
    "client:build": "yarn ng --version && ng build --prod --progress=false --aot=true",
    "client:buildw": "yarn client:build -w",
    "client:test": "yarn client:testw --single-run=true --watch=false",
    "client:testw": "ng test --sourcemaps=false",
    "pree2e": "yarn e2e:docker && wait-on tcp:3305 && yarn server:build && typeorm schema:sync -c test && typeorm migrations:run -c test",
    "e2e": "concurrently -s first --kill-others --exitcrash -p \"[{name}]\" -n \"SERVER,CLIENT\" -c \"bgBlue.bold,bgYellow.bold\" \"yarn e2e:server\" \"yarn e2e:client\"",
    "e2e:server": "yarn server -- test",
    "e2e:client": "ng e2e --progress=false --proxy-config proxy.conf.json",
    "e2e:docker": "docker-compose -f docker-compose.e2e.yml build && docker-compose -f docker-compose.e2e.yml up -d",
    "poste2e": "docker-compose -f docker-compose.e2e.yml kill",
    "db:clean": "typeorm schema:drop",
    "db:sync": "typeorm schema:sync && yarn migrations",
    "migrations": "typeorm migrations:run",
    "migrations:revert": "typeorm migrations:revert",
    "snyk-protect": "snyk protect",
    "prepublish": "yarn snyk-protect"
  },
  "dependencies": {
    "@agm/core": "^1.0.0-beta.1",
    "@angular/animations": "^4.4.6",
    "@angular/cdk": "^2.0.0-beta.12",
    "@angular/common": "4.4.6",
    "@angular/compiler": "4.4.6",
    "@angular/core": "4.4.6",
    "@angular/forms": "4.4.6",
    "@angular/http": "4.4.6",
    "@angular/material": "^2.0.0-beta.12",
    "@angular/platform-browser": "4.4.6",
    "@angular/platform-browser-dynamic": "4.4.6",
    "@angular/router": "4.4.6",
    "@angular/service-worker": "^1.0.0-beta.16",
    "@ngx-translate/core": "^8.0.0",
    "@ngx-translate/http-loader": "2.0.0",
    "angular-font-awesome": "^2.3.7",
    "angulartics2": "^2.4.0",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "chalk": "^2.1.0",
    "cookie-parser": "~1.4.3",
    "core-js": "^2.5.1",
    "cors": "^2.8.4",
    "dragula": "~3.7.2",
    "event-source-polyfill": "0.0.11",
    "express": "~4.16.1",
    "express-session": "^1.15.6",
    "fast-csv": "^2.4.1",
    "flag-icon-css": "^2.8.0",
    "font-awesome": "~4.7.0",
    "handlebars": "^4.0.10",
    "json2csv": "^3.11.2",
    "lodash": "^4.17.4",
    "markdown-to-html-pipe": "1.2.3",
    "marked": "^0.3.6",
    "mkdirp": "~0.5.1",
    "moment": "~2.19.1",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "mysql": "^2.15.0",
    "ng2-dragula": "~1.5.0",
    "node-schedule": "^1.2.5",
    "passport": "~0.4.0",
    "passport-local": "~1.0.0",
    "reflect-metadata": "~0.1.10",
    "request": "^2.83.0",
    "rimraf": "^2.6.2",
    "routing-controllers": "0.7.6",
    "rxjs": "^5.4.3",
    "sendmail": "^1.2.0",
    "serve-favicon": "~2.4.5",
    "session-memory-store": "^0.2.2",
    "typedi": "~0.5.2",
    "typeorm": "^0.1.0",
    "winston": "~2.4.0",
    "zone.js": "0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "~1.5.0",
    "@angular/compiler-cli": "4.4.6",
    "@biesbjerg/ngx-translate-extract": "^2.3.2",
    "@types/bcrypt": "^1.0.0",
    "@types/body-parser": "^1.16.5",
    "@types/bunyan": "^1.8.3",
    "@types/chalk": "~0.4.31",
    "@types/cookie-parser": "~1.4.1",
    "@types/express": "~4.0.37",
    "@types/express-session": "~1.15.3",
    "@types/handlebars": "^4.0.36",
    "@types/jasmine": "~2.6.0",
    "@types/lodash": "^4.14.77",
    "@types/mkdirp": "~0.5.1",
    "@types/morgan": "~1.7.33",
    "@types/multer": "~1.3.3",
    "@types/node": "~8.0.34",
    "@types/node-schedule": "^1.2.0",
    "@types/passport": "~0.3.4",
    "@types/passport-local": "~1.0.31",
    "@types/request": "^2.0.5",
    "@types/rimraf": "^2.0.2",
    "@types/serve-favicon": "~2.2.29",
    "@types/winston": "~2.3.6",
    "codelyzer": "~3.2.0",
    "concurrently": "~3.5.0",
    "jasmine": "^2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jasmine-ts": "^0.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
