{
  "name": "gymsystems",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "npm run server",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "clean": "rm -rf dist",
    "dev": "concurrently \"npm run server:build:watch\" \"npm run server:watch\"",
    "build": "npm run server:build & npm run client:build",
    "deploy": "npm run build && npm run docker",
    "docker": "sh docker-build",
    "docker:dev": "docker-compose build && docker-compose up -d",
    "server": "node ./dist/index.js",
    "server:watch": "nodemon ./dist/index.js -w dist -d .2",
    "server:build": "tsc --project ./server/tsconfig.json",
    "server:build:watch": "npm run server:build -- -w",
    "client": "ng serve --proxy-config proxy.conf.json",
    "client:build": "ng build --prod",
    "client:build:watch": "npm run client:build -- -w",
    "db:clean": "npm run clean && npm run server:build && typeorm schema:drop",
    "db:sync": "npm run clean && npm run server:build && typeorm schema:sync && npm run migrations",
    "migrations": "typeorm migrations:run",
    "migrations:revert": "typeorm migrations:revert"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^2.4.0",
    "@angular/compiler": "^2.4.0",
    "@angular/core": "^2.4.0",
    "@angular/forms": "^2.4.0",
    "@angular/http": "^2.4.0",
    "@angular/platform-browser": "^2.4.0",
    "@angular/platform-browser-dynamic": "^2.4.0",
    "@angular/router": "^3.4.0",
    "body-parser": "^1.16.1",
    "chalk": "^1.1.3",
    "compress": "~0.99.0",
    "cookie-parser": "~1.4.3",
    "core-js": "^2.4.1",
    "dragula": "~3.7.2",
    "eventsource": "~0.2.2",
    "express": "~4.14.0",
    "express-session": "^1.15.1",
    "font-awesome": "~4.7.0",
    "https": "~1.0.0",
    "method-override": "~2.3.7",
    "mkdirp": "~0.5.1",
    "moment": "~2.17.1",
    "morgan": "^1.8.1",
    "mysql": "^2.13.0",
    "ng2-dragula": "~1.3.0",
    "passport": "~0.3.2",
    "passport-local": "~1.0.0",
    "reflect-metadata": "~0.1.9",
    "routing-controllers": "~0.6.7",
    "rxjs": "^5.1.0",
    "serve-favicon": "~2.3.2",
    "typedi": "~0.4.2",
    "typeorm": "^0.0.9",
    "typeorm-routing-controllers-extensions": "^0.0.6",
    "winston": "~2.3.0",
    "zone.js": "^0.7.6"
  },
  "devDependencies": {
    "@angular/cli": "~1.0.0-rc.1",
    "@angular/compiler-cli": "^2.4.0",
    "@types/body-parser": "0.0.33",
    "@types/bunyan": "^0.0.35",
    "@types/chalk": "~0.4.31",
    "@types/cookie-parser": "~1.3.30",
    "@types/express": "~4.0.34",
    "@types/express-session": "0.0.32",
    "@types/jasmine": "2.5.38",
    "@types/method-override": "0.0.29",
    "@types/mkdirp": "~0.3.29",
    "@types/morgan": "~1.7.32",
    "@types/node": "~6.0.60",
    "@types/passport": "~0.3.1",
    "@types/passport-local": "~1.0.29",
    "@types/serve-favicon": "~2.2.28",
    "@types/winston": "~2.2.0",
    "codelyzer": "~2.0.0-beta.4",
    "concurrently": "~3.3.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "nodemon": "~1.11.0",
    "protractor": "~5.1.0",
    "ts-node": "~2.0.0",
    "tslint": "~4.4.2",
    "typescript": "^2.1.6"
  }
}