{
  "name": "gymsystems",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "npm run server",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "clean": "rm -rf dist",
    "dev": "npm run server:build && concurrently --kill-others \"npm run server:build:watch\" \"npm run server:watch\"",
    "build": "npm run server:build & npm run client:build",
    "deploy": "npm run build && npm run docker",
    "docker": "sh docker-build",
    "docker:dev": "docker-compose build && docker-compose up -d",
    "server": "node ./dist/index.js",
    "server:watch": "nodemon --delay .2 ./dist/index.js -w dist",
    "server:build": "tsc --project ./server/tsconfig.json",
    "server:build:watch": "npm run server:build -- -w",
    "client": "ng serve --proxy-config proxy.conf.json",
    "client:build": "ng build --prod",
    "client:build:watch": "npm run client:build -- -w",
    "db:clean": "npm run clean && npm run server:build && typeorm schema:drop",
    "db:sync": "npm run clean && npm run server:build && typeorm schema:sync && npm run migrations",
    "migrations": "typeorm migrations:run",
    "migrations:revert": "typeorm migrations:revert"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^4.0.2",
    "@angular/compiler": "^4.0.2",
    "@angular/core": "^4.0.2",
    "@angular/forms": "^4.0.2",
    "@angular/http": "^4.0.2",
    "@angular/platform-browser": "^4.0.2",
    "@angular/platform-browser-dynamic": "^4.0.2",
    "@angular/router": "^4.0.2",
    "bcrypt": "^1.0.2",
    "body-parser": "^1.17.1",
    "chalk": "^1.1.3",
    "compress": "~0.99.0",
    "cookie-parser": "~1.4.3",
    "core-js": "^2.4.1",
    "dragula": "~3.7.2",
    "eventsource": "~0.2.2",
    "express": "~4.15.2",
    "express-session": "^1.15.2",
    "font-awesome": "~4.7.0",
    "https": "~1.0.0",
    "mkdirp": "~0.5.1",
    "moment": "~2.18.1",
    "morgan": "^1.8.1",
    "mysql": "^2.13.0",
    "ng2-dragula": "~1.3.1",
    "passport": "~0.3.2",
    "passport-local": "~1.0.0",
    "reflect-metadata": "~0.1.10",
    "routing-controllers": "~0.6.12",
    "rxjs": "^5.3.0",
    "serve-favicon": "~2.4.2",
    "typedi": "~0.4.3",
    "typeorm": "^0.0.11",
    "typeorm-routing-controllers-extensions": "^0.0.7",
    "winston": "~2.3.1",
    "zone.js": "^0.8.5"
  },
  "devDependencies": {
    "@angular/cli": "~1.0.0",
    "@angular/compiler-cli": "^4.0.2",
    "@types/bcrypt": "^1.0.0",
    "@types/body-parser": "^1.16.3",
    "@types/bunyan": "^0.0.36",
    "@types/chalk": "~0.4.31",
    "@types/cookie-parser": "~1.3.30",
    "@types/express": "~4.0.35",
    "@types/express-session": "0.0.32",
    "@types/jasmine": "2.5.47",
    "@types/mkdirp": "~0.3.29",
    "@types/morgan": "~1.7.32",
    "@types/node": "~7.0.12",
    "@types/passport": "~0.3.3",
    "@types/passport-local": "~1.0.29",
    "@types/serve-favicon": "~2.2.28",
    "@types/winston": "~2.3.0",
    "codelyzer": "~3.0.0-beta.3",
    "concurrently": "~3.4.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~4.0.0",
    "karma": "~1.6.0",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "nodemon": "~1.11.0",
    "protractor": "~5.1.1",
    "ts-node": "~3.0.2",
    "tslint": "~5.1.0",
    "typescript": "^2.2.2",
    "wait-cli": "~0.5.0"
  }
}