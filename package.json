{
  "name": "gymsystems",
  "version": "0.0.0",
  "description": "Web system for competitive teamgym scoreboarding",
  "homepage": "http://www.gymsystems.org",
  "bugs": "https://github.com/OysteinAmundsen/gymsystems/issues",
  "repository": {
    "url": "https://github.com/OysteinAmundsen/gymsystems",
    "type": "git"
  },
  "author": "oystein.amundsen@gmail.com",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=7.7.0"
  },
  "scripts": {
    "ng": "ng",
    "start": "npm run server",
    "test": "ng test",
    "test:once": "ng test --single-run=true --watch=false",
    "lint": "ng lint",
    "clean": "rm -rf dist",
    "cloc": "cloc client > client/LOC.txt && cloc server > server/LOC.txt && cloc e2e > e2e/LOC.txt",
    "i18n": "ngx-translate-extract -i ./client/app -o ./client/i18n/template.json ./client/i18n/no.json ./client/i18n/en.json --clean --sort",
    "build": "npm run server:build & npm run client:build",
    "deploy": "npm run build && npm run docker",
    "pree2e": "npm run docker:test && wait-on tcp:3305 && npm run server:build && typeorm schema:sync -c test && typeorm migrations:run -c test",
    "e2e": "concurrently -s first --kill-others \"npm run server:test\" \"npm run client:test\"",
    "poste2e": "docker-compose -f docker-e2eTest.yml kill",
    "predev": "npm run server:build",
    "dev": "concurrently --kill-others \"npm run server:build:watch\" \"npm run server:watch\"",
    "docker": "sh docker-build",
    "docker:dev": "docker-compose build && docker-compose up -d",
    "docker:test": "docker-compose -f docker-e2eTest.yml build && docker-compose -f docker-e2eTest.yml up -d",
    "server": "node ./dist/index.js",
    "server:watch": "nodemon --delay .2 ./dist/index.js -w dist",
    "server:build": "tsc --project ./server/tsconfig.json",
    "server:build:watch": "npm run server:build -- -w",
    "server:test": "npm run server -- test",
    "client": "ng serve --proxy-config proxy.conf.json",
    "client:build": "ng build --prod",
    "client:build:watch": "npm run client:build -- -w",
    "client:test": "ng e2e --progress=false --proxy-config proxy.conf.json",
    "db:clean": "typeorm schema:drop",
    "db:sync": "typeorm schema:sync && npm run migrations",
    "migrations": "typeorm migrations:run",
    "migrations:revert": "typeorm migrations:revert"
  },
  "dependencies": {
    "@angular/common": "^4.2.3",
    "@angular/compiler": "^4.2.3",
    "@angular/core": "^4.2.3",
    "@angular/forms": "^4.2.3",
    "@angular/http": "^4.2.3",
    "@angular/platform-browser": "^4.2.3",
    "@angular/platform-browser-dynamic": "^4.2.3",
    "@angular/router": "^4.2.3",
    "@ngx-translate/core": "^7.0.0",
    "@ngx-translate/http-loader": "0.1.0",
    "@types/rimraf": "^0.0.28",
    "angulartics2": "^2.2.2",
    "bcrypt": "^1.0.2",
    "body-parser": "^1.17.2",
    "chalk": "^1.1.3",
    "compress": "~0.99.0",
    "cookie-parser": "~1.4.3",
    "core-js": "^2.4.1",
    "dragula": "~3.7.2",
    "event-source-polyfill": "0.0.9",
    "express": "~4.15.3",
    "express-session": "^1.15.3",
    "font-awesome": "~4.7.0",
    "handlebars": "^4.0.10",
    "lodash": "^4.17.4",
    "markdown-to-html-pipe": "^1.2.1",
    "mkdirp": "~0.5.1",
    "moment": "~2.18.1",
    "morgan": "^1.8.2",
    "multer": "^1.3.0",
    "mysql": "^2.13.0",
    "ng2-dragula": "~1.5.0",
    "node-schedule": "^1.2.3",
    "passport": "~0.3.2",
    "passport-local": "~1.0.0",
    "reflect-metadata": "~0.1.10",
    "rimraf": "^2.6.1",
    "routing-controllers": "0.7.0",
    "rxjs": "^5.4.1",
    "sendmail": "^1.1.1",
    "serve-favicon": "~2.4.3",
    "session-memory-store": "^0.2.2",
    "typedi": "~0.5.2",
    "typeorm": "^0.0.11",
    "winston": "~2.3.1",
    "zone.js": "^0.8.12"
  },
  "devDependencies": {
    "@angular/cli": "~1.2.0",
    "@angular/compiler-cli": "^4.2.3",
    "@biesbjerg/ngx-translate-extract": "^2.3.1",
    "@types/bcrypt": "^1.0.0",
    "@types/body-parser": "^1.16.3",
    "@types/bunyan": "^1.8.0",
    "@types/chalk": "~0.4.31",
    "@types/cookie-parser": "~1.3.30",
    "@types/express": "~4.0.36",
    "@types/express-session": "1.15.0",
    "@types/handlebars": "^4.0.33",
    "@types/jasmine": "2.5.52",
    "@types/lodash": "^4.14.66",
    "@types/mkdirp": "~0.3.29",
    "@types/morgan": "~1.7.32",
    "@types/multer": "1.3.2",
    "@types/node": "~8.0.0",
    "@types/node-schedule": "^1.2.0",
    "@types/passport": "~0.3.3",
    "@types/passport-local": "~1.0.30",
    "@types/serve-favicon": "~2.2.28",
    "@types/winston": "~2.3.3",
    "cloc": "^2.2.0",
    "codelyzer": "~3.1.1",
    "concurrently": "~3.4.0",
    "jasmine-core": "~2.6.4",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "nodemon": "~1.11.0",
    "protractor": "~5.1.2",
    "ts-node": "~3.0.6",
    "tslint": "~5.4.3",
    "typescript": "^2.3.4",
    "wait-on": "^2.0.2"
  }
}
