{
  "name": "gymsystems",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "dev": "concurrently \"npm run build:server:watch\" \"npm run start:server:watch\"",
    "build": "npm run build:server & npm run build:client",
    "build:server": "tsc --project ./server/tsconfig.json",
    "build:server:watch": "npm run build:server -- -w",
    "build:client": "ng build --prod",
    "start": "npm run start:server",
    "start:server": "node ./dist/index.js",
    "start:server:watch": "nodemon ./dist/index.js -w dist -d .2",
    "start:client": "ng serve --proxy-config proxy.conf.json",
    "start:prod": "npm run build & npm run serve",
    "migrations:run": "typeorm migrations:run",
    "migrations:revert": "typeorm migrations:revert",
    "lint": "tslint \"client/**/*.ts\"",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^2.4.0",
    "@angular/compiler": "^2.4.0",
    "@angular/core": "^2.4.0",
    "@angular/forms": "^2.4.0",
    "@angular/http": "^2.4.0",
    "@angular/platform-browser": "^2.4.0",
    "@angular/platform-browser-dynamic": "^2.4.0",
    "@angular/router": "^3.4.0",
    "body-parser": "^1.16.1",
    "chalk": "^1.1.3",
    "compress": "~0.99.0",
    "cookie-parser": "~1.4.3",
    "core-js": "^2.4.1",
    "express": "~4.14.0",
    "express-session": "^1.15.1",
    "font-awesome": "~4.7.0",
    "lodash": "~4.17.4",
    "method-override": "~2.3.7",
    "mkdirp": "~0.5.1",
    "moment": "~2.17.1",
    "morgan": "^1.8.1",
    "mysql": "^2.13.0",
    "passport": "~0.3.2",
    "passport-local": "~1.0.0",
    "reflect-metadata": "~0.1.9",
    "routing-controllers": "~0.6.7",
    "rxjs": "^5.1.0",
    "serve-favicon": "~2.3.2",
    "typedi": "~0.4.2",
    "typeorm": "^0.0.8",
    "typeorm-routing-controllers-extensions": "^0.0.6",
    "winston": "~2.3.0",
    "zone.js": "^0.7.6"
  },
  "devDependencies": {
    "@angular/cli": "~1.0.0-beta.32.3",
    "@angular/compiler-cli": "^2.4.0",
    "@types/body-parser": "0.0.33",
    "@types/bunyan": "^0.0.35",
    "@types/chalk": "~0.4.31",
    "@types/cookie-parser": "~1.3.30",
    "@types/express": "~4.0.34",
    "@types/express-session": "0.0.32",
    "@types/jasmine": "2.5.38",
    "@types/lodash": "~4.14.52",
    "@types/method-override": "0.0.29",
    "@types/morgan": "~1.7.32",
    "@types/node": "~6.0.60",
    "@types/passport": "~0.3.1",
    "@types/passport-local": "~1.0.29",
    "@types/serve-favicon": "~2.2.28",
    "@types/winston": "~2.2.0",
    "codelyzer": "~2.0.0-beta.4",
    "concurrently": "~3.3.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "nodemon": "~1.11.0",
    "protractor": "~5.1.0",
    "ts-node": "~2.0.0",
    "tslint": "~4.4.2",
    "typescript": "^2.1.6"
  }
