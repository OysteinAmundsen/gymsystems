{
  "editor.rulers": [
    180
  ],
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  "editor.trimAutoWhitespace": true,
  "editor.formatOnSave": true,
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.svn": true,
    "**/CVS": true,
    "dist": true
  },
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "tslint.ignoreDefinitionFiles": true,
  "typescript.tsdk": "./node_modules/typescript/lib",
  "debug.node.autoAttach": "on",
  "jest.pathToJest": "./node_modules/.bin/jest",
  "jest.rootPath": "./server/src",
  "angularKarmaTestExplorer.karmaConfFilePath": "./client/src/karma.conf.js",
  "angularKarmaTestExplorer.projectRootPath": "./client",
  "angularKarmaTestExplorer.defaultAngularProjectName": "gymsystems",
  "jestTestExplorer.pathToJest": "./node_modules/.bin/jest",
  "jestTestExplorer.logpanel": true,
  "rest-client.environmentVariables": {
    "$shared": {
      "host": "http://localhost:4200"
    },
    "prod": {
      "host": "https://gymsystems.no"
    }
  }
}
